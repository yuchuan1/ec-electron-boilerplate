{
  "name": "ec-electron-boilerplate",
  "version": "1.0.0",
  "description": "A boilerpate for an electron applciation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuchuan1/ec-electron-boilerplate.git"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "author": "Eddie Chen (yuchuan1@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuchuan1/ec-electron-boilerplate/issues"
  },
  "homepage": "https://github.com/yuchuan1/ec-electron-boilerplate#readme",
  "electronVersion": "0.36.0",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "start": "electron js",
    "build": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion",
    "postinstall": "npm outdated && gulp"
  },
  "files": [
    "src/index.js",
    "src/index.html",
    "src/index.css"
  ],
  "dependencies": {
    "electron-debug": "^0.5.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "del": "^2.2.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.36.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-csslint": "^0.2.0",
    "gulp-cssnano": "^2.0.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^1.1.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-html5-lint": "^1.1.0",
    "gulp-htmllint": "0.0.6",
    "gulp-htmlmin": "^1.3.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-minify-css": "^1.2.2",
    "gulp-mocha": "^2.2.0",
    "gulp-nsp": "^2.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "xo": "^0.12.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
